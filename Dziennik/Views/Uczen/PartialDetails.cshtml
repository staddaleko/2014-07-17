@model IEnumerable<AppDziennik.Models.Ocena_Przedmiot>

<table>
    <tr>
        <th>
            Nazwa przedmiotu
        </th>
        <th>
            Ocena
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nazwa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wartosc)
        </td>
    </tr>
}

</table>
